{
  "name": "bolaji",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:prod": "sudo yarn tsc --project tsconfig.node.json && sudo yarn tsc-alias --project tsconfig.node.json --verbose",
    "migrate:prod": "sudo yarn prisma migrate deploy",
    "exec:waitlist-release": "yarn ts-node -r tsconfig-paths/register src/scripts/send-waitlist-emails.ts",
    "test:emails": "NODE_ENV=development yarn ts-node -r tsconfig-paths/register src/scripts/tests/email.integration.test.ts",
    "migrate:dev": "sudo yarn prisma migrate dev",
    "proxy": "ngrok http --region=us --hostname=bolaji-api-playground.ngrok.io --host-header=rewrite 3400",
    "dev": "NODE_ENV=development nodemon",
    "dev:staging": "NODE_ENV=development nodemon",
    "dev:production": "NODE_ENV=production nodemon",
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node ./dist/main.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/http-errors": "^2.0.5",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/prisma": "^5.0.4",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@prisma/client": "6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "adminjs": "^7.8.17",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.61.2",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.2",
    "get-client-ip": "^3.0.4",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "i18n-iso-countries": "^7.14.0",
    "jsonwebtoken": "^9.0.2",
    "p-limit": "^7.2.0",
    "pino": "^10.1.0",
    "redis": "^5.9.0",
    "resend": "^6.2.2",
    "stripe": "^19.1.0",
